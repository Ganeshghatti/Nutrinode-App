{
  "expo": {
    "name": "NutriNode",
    "slug": "Food_react_native",
    "version": "1.1.3",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/NutriNode.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.NutriNode.food",
      "buildNumber": "1.1.3"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.NutriNode.food",
      "versionCode": 4
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },

    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Merriweather-Regular.ttf"]
        }
      ]
    ]
  }
}
